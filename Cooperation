document.getElementById(" -----Element-ID----- ").innerHTML= product1.productPrice ;
document.getElementById(" -----Element-ID----- ").src = product1.productImgSrc ;
document.getElementById(" -----Element-ID----- ").innerHTML = product1.productName ;








